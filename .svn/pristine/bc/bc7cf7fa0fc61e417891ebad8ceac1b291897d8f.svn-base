package cn.emagsoftware.xfb.dto;

import cn.emagsoftware.frame.bean.BaseRspBean;
import cn.emagsoftware.xfb.pojo.OrderInfo;

import java.util.List;

/**
 * 订单列表
 *
 */
public class OrderListRsp extends BaseRspBean {

    private List<OrderInfo>  orderInfoList;

    private Long page;

    private Integer orderListSize;

    private Integer orderTotal;

    public Integer getOrderListSize() {
        return orderListSize;
    }

    public void setOrderListSize(Integer orderListSize) {
        this.orderListSize = orderListSize;
    }

    public Integer getOrderTotal() {
        return orderTotal;
    }

    public void setOrderTotal(Integer orderTotal) {
        this.orderTotal = orderTotal;
    }

    public List<OrderInfo> getOrderInfoList() {
        return orderInfoList;
    }

    public void setOrderInfoList(List<OrderInfo> orderInfoList) {
        this.orderInfoList = orderInfoList;
    }

    public Long getPage() {
        return page;
    }

    public void setPage(Long page) {
        this.page = page;
    }



}
